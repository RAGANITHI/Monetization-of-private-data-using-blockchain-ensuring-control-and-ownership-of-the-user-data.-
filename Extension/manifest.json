{
    "name": "Ethereum Project",
    "version": "1.0",
    "manifest_version": 2,
  
    "description": "Get your Ethereum",
    "browser_action": {
     "default_icon": "icon.png",
     "default_title": "Give your details get your ethereum",
     "default_popup": "popup.html"
    },
     "background": {
    "page": "background.html"
  },
    "permissions": [
      "webRequest",
     "http://*/*", "https://*/*"
     ]
  }